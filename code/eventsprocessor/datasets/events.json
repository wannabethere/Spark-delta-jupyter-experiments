{
        "$schema": "http://iglucentral.com/schemas/com.snowplowanalytics.self-desc/schema/jsonschema/1-0-0#",
        "description": "Schema for Segment's common event fields, https://segment.com/docs/spec/common/",
        "self": {
                "vendor": "com.segment",
                "customer": "care_solns",
                "name": "event",
                "customertype": "gold",
                "schema": "healthcare",
                "format": "jsonschema",
                "version": "1-0-0"
        },
        "type": "object",
        "title": "Event",
        "properties": {
                "type": {
                        "type": "string",
                        "description":"testing",
                        "maxLength": 512
                },
                "version": {
                        "type": "string",
                        "description":"testing",
                        "maxLength": 128
                },
                "userId": {
                        "type": "string",
                        "description":"testing",
                        "maxLength": 36
                },
                "anonymousId": {
                        "type": "string",
                        "description":"testing",
                        "format": "uuid"
                },
                "channel": {
                        "description":"testing",
                        "enum": [ "server", "browser", "mobile" ]
                },
                "messageId": {
                        "type": "string",
                        "description":"testing",
                        "maxLength": 36
                },
                "projectId": {
                        "type": "string",
                        "description":"testing",
                        "maxLength": 36
                },
                "originalTimestamp": {
                        "type": "string",
                        "format": "date-time"
                },
                "receivedAt": {
                        "type": "string",
                        "format": "date-time"
                },
                "sentAt": {
                        "type": "string",
                        "format": "date-time"
                },
                "timestamp": {
                        "type": "string",
                        "format": "date-time"
                }
                
        },
        "additionalProperties": true
}
